<div class="col-sm-6 col-md-4">
	<div class="thumbnail">
		<h3>{{model}}</h3>
		<p>Year: {{year}}</p>
		<p>Make: {{make}}</p>
		<p>Type: {{type}}</p>
		<p>Location: <a href="{{base_url}}/location/{{location_uid}}">{{location}}</a></p>
	</div>
	<div >
		<form name ="dates" action="{{base_url}}/rent" method="POST" >
			<p>Enter the dates for the rental below</p>
				<ul >
					<li>Start Date: <input type="datetime-local" name="start_date" ></li>
					<li>    End Date: <input type="datetime-local" name="end_date" ></li>
					<input type="submit" value="Submit" >
				</ul>
		</form>
	</div>
</div>